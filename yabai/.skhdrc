alt - e : /opt/homebrew/bin/yabai -m space --layout bsp
alt - s : /opt/homebrew/bin/yabai -m space --layout stack

alt - down : /opt/homebrew/bin/yabai -m window --focus stack.next || /opt/homebrew/bin/yabai -m window --focus south
alt - up : /opt/homebrew/bin/yabai -m window --focus stack.prev || /opt/homebrew/bin/yabai -m window --focus north
ctrl + alt - left : /opt/homebrew/bin/yabai -m window --focus west
ctrl + alt - right : /opt/homebrew/bin/yabai -m window --focus east

alt + shift - 1 : /opt/homebrew/bin/yabai -m window --space 1
alt + shift - 2 : /opt/homebrew/bin/yabai -m window --space 2
alt + shift - 3 : /opt/homebrew/bin/yabai -m window --space 3
alt + shift - 4 : /opt/homebrew/bin/yabai -m window --space 4
alt + shift - 5 : /opt/homebrew/bin/yabai -m window --space 5
alt + shift - 6 : /opt/homebrew/bin/yabai -m window --space 6
alt + shift - 7 : /opt/homebrew/bin/yabai -m window --space 7
alt + shift - 8 : /opt/homebrew/bin/yabai -m window --space 8
alt + shift - 9 : /opt/homebrew/bin/yabai -m window --space 9

alt + shift - k : /opt/homebrew/bin/yabai -m window --swap north
alt + shift - j : /opt/homebrew/bin/yabai -m window --swap south
alt + shift - h : /opt/homebrew/bin/yabai -m window --swap west
alt + shift - l : /opt/homebrew/bin/yabai -m window --swap east

alt - k : /opt/homebrew/bin/yabai -m window --focus north
alt - j : /opt/homebrew/bin/yabai -m window --focus south
alt - h : /opt/homebrew/bin/yabai -m window --focus west
alt - l : /opt/homebrew/bin/yabai -m window --focus east

alt + shift - r : /opt/homebrew/bin/yabai -m space --rotate 90



alt - f : /opt/homebrew/bin/yabai -m window --toggle float
